{
  "name": "react-annex",
  "version": "1.0.0",
  "description":
    "A customisation framework designed to allow for runtime extension of React applications.",
  "main": "src/index.ts",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "npm run lint && npm run build-lib && build-storybook",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "build-storybook": "build-storybook",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "repository": "https://github.com/kyc-chain/react-annex",
  "author": "Dan Cowell <admin@dfcowell.net>",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-notes": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "awesome-typescript-loader": "^4.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "gulp": "^3.9.1",
    "jest": "^23.0.1",
    "prettier": "1.13.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "storybook-addon-jsx": "^5.3.0",
    "storybook-host": "^4.1.5",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@types/storybook__react": "^3.0.7",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true
  }
}
